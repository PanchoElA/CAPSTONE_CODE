<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Add any files you want to include or exclude from cloud backup -->
        <include domain="file" path="." />
        <exclude domain="file" path="temp/" />
    </cloud-backup>
    <device-transfer>
        <!-- Add any files you want to include or exclude from device-to-device transfer -->
        <include domain="file" path="." />
        <exclude domain="file" path="temp/" />
    </device-transfer>
</data-extraction-rules>